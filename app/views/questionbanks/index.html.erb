<h1>Listing questionbanks</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @questionbanks.each do |questionbank| %>
  <tr>
    <td><%= questionbank.title %></td>
    <td><%= questionbank.description %></td>
    <td><%= link_to 'Show', questionbank %></td>
    <td><%= link_to 'Edit', edit_questionbank_path(questionbank) %></td>
    <td><%= link_to 'Destroy', questionbank, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Questionbank', new_questionbank_path %>
